# ЁЯФР уВ╗уВнуГеуГкуГЖуВгшинхоЪ
NODE_ENV=development
PORT=5000

# JWTшинхоЪ
JWT_SECRET=your-super-secure-jwt-secret-key-change-this-in-production
JWT_EXPIRES_IN=24h
JWT_REFRESH_SECRET=your-super-secure-refresh-secret-key

# уГЗуГ╝уВ┐уГЩуГ╝уВ╣шинхоЪ
MONGODB_URI=mongodb://localhost:27017/excel-chatbot
DB_NAME=excel_chatbot

# уВ╗уГГуВ╖уГзуГ│шинхоЪ
SESSION_SECRET=your-super-secure-session-secret-change-this
SESSION_MAX_AGE=86400000

# уГмуГ╝уГИхИ╢щЩРшинхоЪ
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORSшинхоЪ
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# уГнуВ░шинхоЪ
LOG_LEVEL=info
LOG_FILE=logs/app.log

# уВ╗уВнуГеуГкуГЖуВгшинхоЪ
CSRF_SECRET=your-csrf-secret-key
BCRYPT_ROUNDS=12

# SSL/TLSшинхоЪя╝ИцЬмчХкчТ░хвГя╝Й
SSL_KEY_PATH=
SSL_CERT_PATH=

# хдЦщГиAPIшинхоЪя╝ИOpenAIчнЙя╝Й
OPENAI_API_KEY=your-openai-api-key
AI_MODEL=gpt-4

# чЫгцЯ╗уГнуВ░шинхоЪ
AUDIT_LOG_ENABLED=true
AUDIT_LOG_FILE=logs/audit.log