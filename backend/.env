# ЁЯФР уВ╗уВнуГеуГкуГЖуВгшинхоЪ
NODE_ENV=development
PORT=5000

# JWTшинхоЪ
JWT_SECRET=excel-chatbot-super-secure-jwt-secret-key-change-this-in-production-2024
JWT_EXPIRES_IN=24h
JWT_REFRESH_SECRET=excel-chatbot-super-secure-refresh-secret-key-2024

# уГЗуГ╝уВ┐уГЩуГ╝уВ╣шинхоЪя╝ИщЦЛчЩ║чФия╝Й
MONGODB_URI=mongodb://localhost:27017/excel-chatbot
DB_NAME=excel_chatbot

# уВ╗уГГуВ╖уГзуГ│шинхоЪ
SESSION_SECRET=excel-chatbot-super-secure-session-secret-change-this-2024
SESSION_MAX_AGE=86400000

# уГмуГ╝уГИхИ╢щЩРшинхоЪ
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORSшинхоЪ
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5000

# уГнуВ░шинхоЪ
LOG_LEVEL=info
LOG_FILE=logs/app.log

# уВ╗уВнуГеуГкуГЖуВгшинхоЪ
CSRF_SECRET=excel-chatbot-csrf-secret-key-2024
BCRYPT_ROUNDS=12

# чобчРЖшАЕшинхоЪ
ADMIN_PASSWORD=SecureAdmin123!

# хдЦщГиAPIшинхоЪя╝ИуГЗуГвчФия╝Й
OPENAI_API_KEY=your-openai-api-key-here
AI_MODEL=gpt-4

# чЫгцЯ╗уГнуВ░шинхоЪ
AUDIT_LOG_ENABLED=true
AUDIT_LOG_FILE=logs/audit.log